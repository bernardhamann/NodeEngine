@import 'rupture'

html {
    font-size: 1em;
    color: #535d65;
    font-family: 'Asap', sans-serif;
    word-wrap: break-word;
}
body {
    background: #eae6e6;
    padding: 0;
    margin: 0;
    display: block;
}
a {
    color: #00b7ff;
}
a:hover {
    color: #800080;
}
h1 {
    margin: 0;
    padding: 0;
}


.ne-row{
  lost-utility: clearfix
}

.ne-cols-50{
  lost-column: 50/100 2
}

